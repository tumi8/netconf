                </present>
            </fileset>
        </replaceregexp>
    </target>
    <target name="j2ee13.modifications" depends="errorhandler.modifications,j2ee13.web.xml,j2ee13.index.jsp,j2ee13.jspf"/>
    <target name="errorhandler.modifications" depends="extract.war">
        <move file="${dist.jar.dir}/tmp/WEB-INF/web.xml" tofile="${dist.jar.dir}/tmp/WEB-INF/web.before.errorhandler"/>
        <xslt in="${dist.jar.dir}/tmp/WEB-INF/web.before.errorhandler" out="${dist.jar.dir}/tmp/WEB-INF/web.xml" style="nbproject/export-web-xml.xsl" force="true">
            <xmlcatalog>
                <dtd publicId="-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" location="file://${jscreator.sjsas8pe.home}/lib/dtds/web-app_2_3.dtd"/>
            </xmlcatalog>
        </xslt>
    </target>
    <target name="pluto-hack-web-xml-dist" depends="extract.war,j2ee13.init" if="project.supports.portlets" unless="j2ee13">
        <move file="${dist.jar.dir}/tmp/WEB-INF/web.xml" tofile="${dist.jar.dir}/tmp/WEB-INF/web.before.pluto"/>
        <move tofile="${dist.jar.dir}/tmp/WEB-INF/web.xml" file="${dist.jar.dir}/tmp/WEB-INF/web.before.pluto">
            <filterchain>
                <tokenfilter>
                    <replaceregex pattern="&lt;web-app.*version=.*&gt;" replace="&lt;web-app version=&quot;2.4&quot;&gt;" flags="g"/>
                </tokenfilter>
            </filterchain>
        </move>
    </target>
    <target name="jsCreatorModifyWar" depends="errorhandler.modifications,j2ee13.modifications,pluto-hack-web-xml-dist">
        <delete quiet="true">
            <fileset dir="${dist.jar.dir}/tmp/WEB-INF/" includes="web.before.*"/>
        </delete>
        <jar jarfile="${dist.war}" compress="${jar.compress}" basedir="${dist.jar.dir}/tmp"/>
        <delete dir="${dist.jar.dir}/tmp" quiet="true"/>
    </target>
