    <target name="jsCreatorDist" depends="init,compile,compile-jsps,-do-compile-single-jsp,pluto-hack-web-xml-run,portlet-container-deploy" if="j2ee.deploy.war">
        <antcall target="dist"/>
    </target>
    <target name="pluto-hack-web-xml-run" depends="init,compile,compile-jsps,-do-compile-single-jsp" if="project.supports.portlets">
        <move file="${build.web.dir}/WEB-INF/web.xml" tofile="${build.web.dir}/WEB-INF/web.before.pluto"/>
        <move tofile="${build.web.dir}/WEB-INF/web.xml" file="${build.web.dir}/WEB-INF/web.before.pluto">
            <filterchain>
                <tokenfilter>
                    <replaceregex pattern="&lt;web-app.*version=.*&gt;" replace="&lt;web-app version=&quot;2.4&quot;&gt;" flags="g"/>
                </tokenfilter>
            </filterchain>
        </move>
    </target>
    <target name="portlet-container-deploy" depends="init" if="deploy.portlet.container">
        <portletcontainerDeploy portletDescriptorFile="${portlet.deployment.descriptor}" portletProjectName="${portlet.project.name}"/>
    </target>
    <target name="verify" depends="init,dist">
        <nbverify file="${dist.war}"/>
